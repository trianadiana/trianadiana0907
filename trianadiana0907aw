#[contract]
mod HeyStarknet {
    use starknet::get_caller_address;
    use starknet::ContractAddress;


    #[event]
    fn Hey(from: ContractAddress, value: felt252) {}


    #[external]
    fn Hey(message: felt252) {
        let caller = get_caller_address();
        Hello(caller, message);
    }

}
